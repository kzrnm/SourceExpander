{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": true,
    "properties": {
        "enabled": {
            "type": "boolean",
            "default": true
        },
        "embedding-type": {
            "description": "embedding type",
            "type": "string",
            "default": "gzip-base32768",
            "enum": [
                "raw",
                "gzip-base32768"
            ]
        },
        "exclude-attributes": {
            "description": "attribute full name that remove on embedding",
            "type": "array",
            "items": {
                "description": "attribute full name that remove on embedding",
                "type": "string"
            }
        },
        "add-trivia-kinds": {
            "description": "A list of Microsoft.CodeAnalysis.CSharp.SyntaxKind with LeadingTrivia/Trailing trivia",
            "type": "array",
            "items": {
                "description": "Microsoft.CodeAnalysis.CSharp.SyntaxKind with LeadingTrivia/Trailing trivia",
                "type": "string"
            }
        }
    }
}
