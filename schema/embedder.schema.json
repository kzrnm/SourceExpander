{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": true,
    "properties": {
        "enabled": {
            "type": "boolean",
            "default": true
        },
        "enable-minify": {
            "type": "boolean",
            "description": "[experimental]minify embedded source code",
            "default": false
        },
        "embedding-source-class": {
            "type": "object",
            "description": "(For debug)Embedding source code as class",
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "default": false
                },
                "if-directive": {
                    "description": "#if directive name",
                    "type": "string",
                    "default": "DEBUG"
                }
            }
        },
        "remove-conditional": {
            "description": "Remove method Conditional(\"{Value}\")",
            "type": "array",
            "default": [],
            "items": {
                "description": "Remove method Conditional(\"{Value}\")",
                "type": "string"
            }
        },
        "embedding-type": {
            "description": "embedding type",
            "type": "string",
            "default": "gzip-base32768",
            "enum": [
                "raw",
                "gzip-base32768"
            ]
        },
        "exclude-attributes": {
            "description": "attribute full name that remove on embedding",
            "type": "array",
            "items": {
                "description": "attribute full name that remove on embedding",
                "type": "string"
            }
        }
    }
}